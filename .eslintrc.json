{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "consistent-return": 0,
    "no-console": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "no-continue": "off"
  },
  "ignorePatterns": ["node_modules"]
}

